  Sylvain Le Gall <gildor@debian.org>
Build-Depends:
  ocaml-nox (>= 3.11.1-3~),
  ocaml-findlib,
  ocaml-base-nox,
  libtype-conv-camlp4-dev,
  libounit-ocaml-dev,
  libfileutils-ocaml-dev (>= 0.4.0),
  dh-ocaml (>= 0.9~),
  debhelper (>= 7.0.50~),
